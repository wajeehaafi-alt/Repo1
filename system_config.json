  "repositories": {
    "route1": {
      "source": {
        "owner": "ptechfusion19",
        "name": "Repo-A"
      },
      "target": {
        "owner": "ptechfusion19",
        "name": "Repo-B"
      }
    },
    "route2": {
      "source": {
        "owner": "ptechfusion19",
        "name": "Org_Testing_Repo"
      },
      "target": {
        "owner": "Ramzanx0553",
