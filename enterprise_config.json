{
  "enterprise": {
    "name": "Workflow Automation Enterprise",
    "version": "5.0.0",
    "edition": "enterprise",
    "license": "enterprise"
  },
  "infrastructure": {
    "deployment": {
      "platform": "kubernetes",
      "namespace": "workflow-automation",
      "replicas": 3,
      "strategy": "blue-green",
      "auto_scaling": {
        "enabled": true,
        "min_replicas": 3,
        "max_replicas": 20,
        "target_cpu": 70,
        "target_memory": 80
      }
    },
    "high_availability": {
      "enabled": true,
      "multi_zone": true,
      "backup_enabled": true,
      "backup_interval_hours": 6,
      "retention_days": 30
    },
    "monitoring": {
      "prometheus": {
        "enabled": true,
        "retention_days": 90
      },
      "grafana": {
        "enabled": true,
        "dashboards": ["workflow-overview", "sync-metrics", "error-tracking"]
      },
      "alerting": {
        "enabled": true,
        "channels": ["email", "slack", "pagerduty", "webhook"]
      }
    }
  },
  "security": {
    "authentication": {
      "method": "oauth2",
      "provider": "github",
      "sso_enabled": true,
      "mfa_required": true
    },
    "encryption": {
      "at_rest": true,
      "algorithm": "AES-256",
      "in_transit": true,
      "tls_version": "1.3"
    },
    "secrets_management": {
      "provider": "vault",
      "rotation_enabled": true,
      "rotation_interval_days": 30,
      "audit_logging": true
    },
    "access_control": {
      "rbac_enabled": true,
      "ip_whitelist": [],
      "rate_limiting": {
        "enabled": true,
        "requests_per_minute": 1000
      }
    }
  },
  "repositories": {
    "routes": [
      {
        "id": "route1",
        "name": "Repo-A ↔ Repo-B",
        "source": {
          "owner": "ptechfusion19",
          "name": "Repo-A",
          "credential": "Account 4",
          "webhook_secret": "${WEBHOOK_SECRET_1}"
        },
        "target": {
          "owner": "ptechfusion19",
          "name": "Repo-B",
          "credential": "Account 4"
        },
        "enabled": true,
        "priority": "high"
      },
      {
        "id": "route2",
        "name": "Org_Testing ↔ Per_Testing",
        "source": {
          "owner": "ptechfusion19",
          "name": "Org_Testing_Repo",
          "credential": "Account 4",
          "webhook_secret": "${WEBHOOK_SECRET_2}"
        },
        "target": {
          "owner": "Ramzanx0553",
          "name": "Per_Testing_Repo",
          "credential": "Account 4"
        },
        "enabled": true,
        "priority": "medium"
      },
      {
        "id": "route3",
        "name": "Repo1 ↔ Repo2",
        "source": {
          "owner": "Ramzanx0553",
          "name": "Repo2",
          "credential": "Account 4",
          "webhook_secret": "${WEBHOOK_SECRET_3}"
        },
        "target": {
          "owner": "wajeehaafi-alt",
          "name": "Repo1",
          "credential": "Account 5"
        },
        "enabled": true,
        "priority": "high"
      }
    ]
  },
  "sync": {
    "settings": {
      "enabled": true,
      "auto_merge": false,
      "create_pr": true,
      "branch_prefix": "sync-from",
      "max_files_per_commit": 100,
      "timeout_seconds": 600,
      "retry_attempts": 5,
      "retry_delay_seconds": 10,
      "parallel_processing": true,
      "max_parallel_files": 5
    },
    "filters": {
      "ignore_patterns": [
        "*.log",
        "*.tmp",
        ".git/**",
        "node_modules/**",
        ".env",
        "*.secret"
      ],
      "max_file_size_mb": 50,
      "allowed_extensions": [
        ".py", ".js", ".html", ".css", ".cpp", ".md", 
        ".json", ".yaml", ".yml", ".txt", ".sql", ".xml"
      ]
    },
    "validation": {
      "enabled": true,
      "check_file_integrity": true,
      "verify_sha": true,
      "validate_content": false
    }
  },
  "compliance": {
    "audit_logging": {
      "enabled": true,
      "retention_days": 365,
      "encrypted": true
    },
    "data_retention": {
      "execution_logs_days": 90,
      "file_sync_history_days": 180,
      "pr_history_days": 365
    },
    "gdpr": {
      "enabled": true,
      "data_anonymization": true,
      "right_to_deletion": true
    }
  },
  "support": {
    "tier": "enterprise",
    "sla": "99.9%",
    "response_time_hours": 1,
    "support_channels": ["email", "phone", "chat", "ticket"]
  }
}

